{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "5fb687d9-fd50-4b16-bcbd-e9486272b0d8",
        "SanitizeTinyMce": true
      },
      "ModelsBuilder": {
        "UpgradeUnattended": true,
        "ModelsMode": "SourceCodeManual",
        "ModelsNamespace": "Trivical.Core.Models",
        "ModelsDirectory": "~/../Trivical.Core/Models/Generated",
        "AcceptUnsafeModelsDirectory": true,
        "DebugLevel": 0
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      }
    }
  }
}
